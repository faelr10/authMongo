generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["mongoDb"]
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}


model Policy {
  id       String   @id @default(dbgenerated()) @map("_id") @db.ObjectId
  Action   String[]
  Effect   String
  Resource String[]
  Sid      String
}

model User {
  id           String @id @default(dbgenerated()) @map("_id") @db.ObjectId
  balance      Int
  cpf          String
  isAuthorized String
  name         String
  password     String
}
